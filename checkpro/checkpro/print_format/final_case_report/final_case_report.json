 "doctype": "Print Format",
 "font": "Default",
 "html": "<meta name=\"pdfkit-orientation\" content=\"Portrait\" />\n<div class=\"container\">\n\n    <header id=\"header-html\">\n        <div class=\"row\">\n            <div class=\"col-xs-12\">\n                {% set letter_head = frappe.get_doc(\"Letter Head\",\"TeamPRO Letter Head\") %}\n                {% if letter_head %}\n                {{ letter_head.content }}\n\n                {% endif %}\n            </div>\n        </div>\n    </header>\n        <h4>\n            <center> <b> BACKGROUND CHECK REPORT </b> </center>\n        </h4>\n\n    \n<div class=\"row\" >\n<div class=\"col-xs-9\" >\n      <p style=\"color:steelBlue;\">CASE SUMMARY</p>\n            <p style=\"color:orange;\">A. Client & Case Detail</p>\n            \n            {% set cust = frappe.get_doc(\"Client\", {\"name\":doc.client}) %}\n            \n            <table border=\"1\" width=\"500%\" class=\"print-friendly\"> \n                <tr>\n                    <td style=\"background-color:steelblue;color:white\" width=\"40%\">Company Name </td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"4\">Employee Name </td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"2\">EMP ID </td>\n                </tr>\n                <tr>\n                    <td rowspan=\"2\"> {{cust.name}}</td>\n                    <td colspan=\"4\"> {{doc.case_name}}</td>\n\n                    <td colspan=\"2\">{{doc.client_employee_code}}</td>\n\n                </tr>\n                <tr>\n                    <td style=\"background-color:steelblue;color:white\" width=\"10%\">DOB</td>\n                    <td width=\"25%\">{{doc.date_of_birth}}</td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"1\">Age</td>\n                    <td colspan=\"1\">{{doc.age}}</td>\n                    <td style=\"background-color:steelblue;color:white\" width=\"15%\" colspan=\"1\">Gender</td>\n                    <td colspan=\"1\">{{doc.case_gender}}</td>\n                </tr>\n                <tr>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"2\">Company Address</td>\n\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"5\">Employee Address</td>\n\n                </tr>\n                <tr>\n                    <td colspan=\"2\">{{cust.address}}</td>\n                    <td colspan=\"5\">{{doc.address}}</td>\n                </tr>\n                <tr>\n                    <td style=\"background-color:steelblue;color:white\" width=\"25%\">Submitted To:</td>\n                    <td style=\"background-color:steelblue;color:white\" width=\"25%\">Start Date</td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"2\">End Date</td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"3\">TAT Days</td>\n                </tr>\n                <tr>\n                    <td width=\"30%\">{{doc.hr_name}}</td>\n                    <td width=\"25%\">{{doc.date_of_initiating}}</td>\n                    <td colspan=\"2\">{{doc.end_date}}</td>\n                    <td colspan=\"5\">{{doc.tat}}</td>\n\n                </tr>\n                <tr>\n                    <td width=\"30%\">{{doc.hr_designation}}</td>\n                    <td width=\"35%\">THIS ID</td>\n                    <td colspan=\"5\">{{cust.short_code}}</td>\n\n                </tr>\n            </table>\n        </div>\n\n        <div class=\"col-xs-3\">\n            <p align=\"center\" style=\"color:steelBlue\">Overall Status</p>\n            {% set icon_bvs = frappe.get_doc(\"Letter Head\",\"TeamPRO BVS\") %}\n            {% if doc.case_report==\"Positive\"%}\n\n            <img src=\"/files/Positive.png\" width=\"70%\" height=100px>\n            {% elif doc.case_report==\"Alert\" %}\n            <img src=\"/files/Negative.png\" width=\"70%\" height=100px>\n            {% elif doc.case_report==\"Dilemma\" %}\n            <img src=\"/files/Dilemma.png\" width=\"70%\" height=100px>\n            {% elif doc.case_report==\"Interim\" %}\n            <img src=\"/files/Interim.png\" width=\"70%\" height=100px>\n\n            {% endif %}\n     <table border=\"1\" width=\"30%\"  class=\"print-friendly\">\n    <tr>\n                    <td style=\"background-color:steelblue;color\" colspan=\"1\"><b>CC</b></td>\n                    <td style=\"background-color:steelblue;color\" colspan=\"1\"><b>Description</b></td>\n                </tr>\n                <tr>\n                    <td colspan=\"1\"><b>Positive </b></td>\n                    \n                    <td colspan=\"1\"><b>Good</b></td>\n              \n                </tr>\n                 <tr>\n                    <td colspan=\"1\"><b>Alert</b></td>\n                    \n                    <td colspan=\"1\"><b>bad</b></td>\n              \n                </tr>\n                 <tr>\n                    <td colspan=\"1\"><b>Dilemma</b></td>\n                    \n                    <td colspan=\"1\"><b>neutral</b></td>\n              \n                </tr>\n                 <tr>\n                    <td colspan=\"1\"><b>Insufficient </b></td>\n                    \n                    <td colspan=\"1\"><b>Insufficient</b></td>\n              \n                </tr>\n                </table>\n\n        </div>\n        <div align=\"right\">\n            \n        <!--<img src=\"/files/description.png\"  >-->\n         \n        </div>\n    \n</div>\n\n    <div class=\"row\">\n\n   \n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <br>\n             <p style=\"color:orange;\">B. Check Status</p>\n           <table border=\"1\" width=\"300%\"  class=\"print-friendly\">\n                <tr>\n                    <td style=\"background-color:steelblue;color:white\" colspan=3>Checks Type</td>\n\n                    <td style=\"background-color:steelblue;color:white\" colspan=1>Units</td>\n\n                    <td style=\"background-color:steelblue;color:white\" colspan=2>Status</td>\n                </tr>\n                {% for row in doc.checkwise_report %}\n                <tr>\n                    <td colspan=\"3\">{{row.checks}}</td>\n                    <td colspan=1>1</td>\n                    {% if row.report_status == \"Pending\" %}\n                    <td colspan=2>Pending</td>\n                    {% elif row.report_status == \"Positive\" %}\n                    <td style=\"color:Green;\" colspan=2>Positive</td>\n                    {% elif row.report_status == \"Alert\" %}\n                    <td style=\"color:Red\" colspan=2>Alert</td>\n                    {% elif row.report_status == \"Dilemma\" %}\n                    <td style=\"color:Yellow\" colspan=2>Dilemma</td>\n                    {% elif row.report_status == \"Interim\" %}\n                    <td style=\"color:SkyBlue\" colspan=2>Interim</td>\n                    {% endif %}\n\n                </tr>\n                {%- endfor -%}\n            </table>\n        </div>\n    </div>\n <br>\n    <p style=\"color:orange;\">C. Verification Remark</p>\n    <table border=\"1\" width=\"300%\"  class=\"print-friendly\">\n        <tr>\n            <td style=\"background-color:steelblue;color:white\" colspan=3>Remarks</td>\n\n        </tr>\n\n        <tr>\n            {% if doc.remarks_green %}\n            <td>{{doc.remarks_green}}</td>\n            {% elif doc.remarks_red %}\n            <td>{{doc.remarks_red}}</td>\n            {% elif doc.remarks_amber %}\n            <td>{{doc.remarks_amber}}</td>\n            {% elif doc.remarks_interim %}\n            <td>{{doc.remarks_interim}}</td>\n            {% endif %}\n\n        </tr>\n\n    </table>\n\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n             <br> \n            <p style=\"color:orange;\">D.Signatures</p>\n            <!--<center><img src=\"/files/sign-page0001 (1).png\" width=\"100%\"></center>-->\n            <table  class=\"print-friendly\" border=\"1\" >\n    <tr>\n                    <td style=\"background-color:steelblue;color\" colspan=\"3\"><b>Check Executive</b></td>\n                    <td style=\"background-color:steelblue;color\" colspan=\"3\"><b>Quality Review</b></td>\n                    <td style=\"background-color:steelblue;color\" colspan=\"3\"><b>Report Review</b></td>\n                </tr>\n                <tr>\n                    <td colspan=\"3\"><b>Kartika </b></td>\n                    \n                    <td colspan=\"3\"><b>Sangeetha SBMK</b></td>\n                   \n                    <td colspan=\"3\"><b>Dinesh K Babu</b></td>\n              \n                </tr>\n                 <tr>\n                    \n                    <td colspan=\"3\"><b>Deputy manager</b></td>\n                  \n                    <td colspan=\"3\"><b>Operations Head</b></td>\n                    \n                    <td colspan=\"3\"><b>Director</b></td>\n                </tr>\n            \n </table>\n            </div>\n    </div>\n   <p style=\"page-break-before: always;\">&nbsp;</p>\n        \n         <div class=\"row\">\n             \n        <div class=\"col-xs-12\">\n          \n             {% for c in doc.checkwise_report %}\n\n            {% set check = c.checks%}\n\n            <h4>\n                 \n                <center>BACKGROUND CHECK REPORT</center>\n            </h4>\n           \n            <h5 style=\"color:steelBlue;text-align:center;\"> Check Report </h5>\n            <h5 style=\"color:steelBlue;text-align:center;\"> <b>{{check}} </b> </h5>\n\n            <p style=\"color:orange;\">A.Case Details</p>\n            {% set e = frappe.get_all(check,{\"name\":c.check_id},[\"*\"]) %}\n    \n            <table border=\"1\" width=\"300%\"  class=\"print-friendly\">\n\n                <tr>\n\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"3\">Name </td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"2\">EMP ID</td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"3\">DOB</td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"2\">AGE </td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"2\">Gender</td>\n                </tr>\n                <tr>\n                    <td colspan=\"3\"> {{e[0].name1}}</td>\n                    <td colspan=\"2\"> {{e[0].client_employee_code}}</td>\n                    <td colspan=\"3\">{{e[0].date_of_birth}}</td>\n                    <td colspan=\"2\"> {{e[0].age}}</td>\n                    <td colspan=\"2\">{{e[0].gender}}</td>\n                </tr>\n                <tr>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"2\">THIS ID </td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"2\">Start Date</td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"2\">End Date</td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"2\">Report Date</td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"2\">TAT</td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"2\">Units</td>\n                </tr>\n                <tr>\n                    <td colspan=\"2\"> {{e[0].client_shortcode}}</td>\n                    <td colspan=\"2\"> {{e[0].date_of_entry}}</td>\n                    <td colspan=\"2\">{{e[0].date_of_initiation}}</td>\n                    <td colspan=\"2\"> {{e[0].date_of_completion}}</td>\n                    <td colspan=\"2\">{{e[0].ce_tatdays}}</td>\n                    <td colspan=\"2\">1</td>\n\n                </tr>\n\n            </table>\n<br>\n            <table  class=\"print-friendly\" border=\"1\">\n\n                <tr>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"3\"><b>Check Status</b></td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"3\"><b>Observation</b></td>\n                </tr>\n                <tr>\n                     \n                   \n                 {% if doc.case_report==\"Positive\"%}\n\n           <td colspan=\"3\"> <img src=\"/files/Positive.png\" width=\"20%\"></td>\n            {% elif doc.case_report==\"Alert\" %}\n           <td colspan=\"3\">  <img src=\"/files/Negative.png\" width=\"20%\"></td>\n            {% elif doc.case_report==\"Dilemma\" %}\n          <td colspan=\"3\">   <img src=\"/files/Dilemma.png\" width=\"20%\"></td>\n            {% elif doc.case_report==\"Interim\" %}\n          <td colspan=\"3\">  <img src=\"/files/Interim.png\" width=\"20%\"></td>\n\n            {% endif %}\n            \n             <td colspan=\"3\"><b>{{e[0].observation}}</b></td>\n</tr>\n              \n                <tr>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"3\"><b>Straus</b></td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"3\"><b>Attachment</b></td>\n                </tr>\n\n                <tr>\n                    <td colspan=\"3\"><b>{{e[0].report_status}}</b></td>\n                    <td colspan=\"3\"><b>{{e[0].attachment}}</b></td>\n                </tr>\n\n            </table>\n            \n            \n            <br>\n            \n            <p style=\"color:orange;\">Check Detailed Report</p>\n            <table border=\"1\" width=\"300%\"  class=\"print-friendly\">\n             \n                <tr>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"3\"><b>Description</b></td>\n                    <td  style=\"background-color:steelblue;color:white\" colspan=\"3\"><b>EPI</b></td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"3\"><b>VPD</b></td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"3\"><b>Status</b></td>\n\n                </tr>\n              \n               {% set entry_list = [] %}\n                {% set verify_list = [] %}\n                {% set var_list = [] %}\n                {% set line_list = [] %}\n                {% for r,i in e[0].items() %}\n            \n              {% set c = r.split('_')[:-2] %}\n                {% set sep= c|join(\"_\") %}\n\n                {% if r.partition('_')[0]=='epi' %}\n                <!--{{ entry_list.append({r:i}) }}-->\n\n                {% set label= r|replace(\"epi_\", \"\") %}\n\n                <!--{{ var_list.append(label) }}-->\n\n\n\n                {% endif %}\n                {% if r.partition('_')[0]=='vpi'%}\n                <!--{{ verify_list.append({r:i}) }}-->\n                {% endif %}\n                {% if r == sep+'_line_status'%}\n                <!--{{ line_list.append({r:i}) }}-->\n                {% endif %}\n       {% endfor %}\n                {% for r in var_list %}\n\n                <tr>\n                    <td colspan=\"3\">{{r|capitalize | replace(\"_\",\" \")}}</td>\n                    <td colspan=\"3\">{{ entry_list[var_list.index(r)][\"epi_\"+r]}} </td>\n                    <td colspan=\"3\">{{ verify_list[var_list.index(r)][\"vpi_\"+r] }}</td>\n                    <td colspan=\"3\">{{ line_list[var_list.index(r)][r+\"_line_status\"] }}</td>\n\n                </tr>\n{% endfor %}\n            </table>\n              \n\n<br> <p style=\"page-break-before: always;\">&nbsp;</p>\n\n<table  class=\"print-friendly\" border=\"1\" >\n    <tr>\n                    <td style=\"background-color:steelblue;color:white\"  colspan=\"3\"><b>Check Executive</b></td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"3\"><b>Quality Review</b></td>\n                    <td style=\"background-color:steelblue;color:white\" colspan=\"3\"><b>Report Review</b></td>\n                </tr>\n                <tr>\n                    <td colspan=\"3\"><b>{{e[0].entered_by}}</b></td>\n                    \n                    <td colspan=\"3\"><b>{{e[0].verified_by}}</b></td>\n                   \n                    <td colspan=\"3\"><b>{{e[0].approved_by}}</b></td>\n              \n                </tr>\n                 <tr>\n                    \n                    <td colspan=\"3\"><b>{{e[0].entry_designation}}</b></td>\n                  \n                    <td colspan=\"3\"><b>{{e[0].designation}}</b></td>\n                    \n                    <td colspan=\"3\"><b>{{e[0].approved_designation}}</b></td>\n                </tr>\n            \n </table>\n\n\n          {% endfor %}\n          \n         \n        </div>\n       \n    </div>\n\n\n    \n    <footer id=\"footer-html\">\n\n        {{letter_head.footer}}\n\n\n    </footer>\n\n</div>\n \n\n</div>",
